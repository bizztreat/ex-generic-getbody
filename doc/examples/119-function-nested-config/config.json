{
    "parameters": {
        "api": {
            "baseUrl": {
                "function": "concat",
                "args": [
                    "http://",
                    {
                        "attr": "server.address"
                    },
                    ":",
                    {
                        "attr": "server.port"
                    },
                    "/087-function-baseurl/"
                ]
            }
        },
        "config": {
            "debug": true,
            "outputBucket": "mock-server",
            "server": {
                "address": "mock-server",
                "port": "80"
            },
            "jobs": [
                {
                    "endpoint": "users",
                    "dataType": "users"
                }
            ]
        }
    }
}
